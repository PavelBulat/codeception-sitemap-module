{
    "name": "portrino/codeception-sitemap-module",
    "description": "Parse and validate sitemap.xml files",
    "authors": [
        {
            "name": "Andr√© Wuttig",
            "email": "wuttig@portrino.de"
        }
    ],
    "require": {
        "codeception/codeception": "~2.3",
        "vipnytt/sitemapparser": "~1.0.3",
        "portrino/phpunit-xsdvalidation": "1.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.4",
        "squizlabs/php_codesniffer": "~3.0.2"
    },
    "autoload": {
        "psr-4": {
            "Codeception\\Module\\": "src"
        }
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/portrino/phpunit-xsdvalidation.git"
        }
    ],
    "config": {
        "vendor-dir": "build/vendor",
        "bin-dir": "build/bin"
    }
}
